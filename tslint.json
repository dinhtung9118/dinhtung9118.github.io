{
  "extends":[
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [true, "log", "error"],
    "prefer-for-of": false,
    "variable-name": {
      "options": [
        "allow-pascal-case",
        "allow-snake-case",
        "ban-keywords",
        "check-format"
      ]
    },
    "ban-types": false,
    "no-unused-expression": false,
    "jsx-no-lambda": false,
    "class-name": false,
    "no-var-requires": false,
    "jsx-boolean-value": [true, "never"]
  },
  "jsRules": {}
}
